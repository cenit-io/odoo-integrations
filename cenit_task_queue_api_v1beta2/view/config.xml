<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="view_cenit_task_queue_api_v1beta2_settings" model="ir.ui.view">
            <field name="name">Lets you access a Google App Engine Pull Task Queue over REST. settings</field>
            <field name="model">cenit.task_queue_api_v1beta2.settings</field>
            <field name="arch" type="xml">
                <form string="Configure Lets you access a Google App Engine Pull Task Queue over REST." class="oe_form_configuration">
                    <header>
                        <button string="Apply" type="object"
                                name="execute" class="oe_highlight"/>
                        or
                        <button string="Cancel" type="object"
                                name="cancel" class="oe_link"/>
                    </header>
                    <div>
                        <group string="Lets you access a Google App Engine Pull Task Queue over REST. settings">
                        </group>
                    </div>
                </form>
            </field>
        </record>

        <record id="action_cenit_task_queue_api_v1beta2_settings" model="ir.actions.act_window">
            <field name="name">Lets you access a Google App Engine Pull Task Queue over REST. settings</field>
            <field name="res_model">cenit.task_queue_api_v1beta2.settings</field>
            <field name="view_mode">form</field>
            <field name="target">inline</field>
            <field name="view_id" ref="view_cenit_task_queue_api_v1beta2_settings"/>
        </record>

        <menuitem id="menu_cenit_task_queue_api_v1beta2_settings"
            parent="cenit_base.menu_cenit_integrations_settings"
            sequence="1" action="action_cenit_task_queue_api_v1beta2_settings"/>

        <menuitem id="menu_cenit_task_queue_api_v1beta2_settings_alt"
            parent="cenit_base.menu_cenit_integrations_settings_alt"
            sequence="1" action="action_cenit_task_queue_api_v1beta2_settings"/>

    </data>
</openerp>
