            <field name="res_model">cenit.labs64_netlicensing_2_x.settings</field>
            <field name="view_id" ref="wizard_cenit_labs64_netlicensing_2_x_install"/>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
